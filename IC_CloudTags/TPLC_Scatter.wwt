        code : |1-
         
         gclATag is cTag
         
         // CSS header
         gsCssHeader is string = [
         	<style>
         	ul#TagLinkStyle{
         	list-style-type: none ;
         	/* Delete the margin, padding and tags from <ul> */
         	}
         	ul#TagLinkStyle li
         	{
         	margin: 0 0 5px 0 ;
         	padding: 0 ;
         	float: left;
         	/* Delete the padding from <li> and define a bottom margin of 5px */
         	}
         	
         	ul#TagLinkStyle li a
         	{
         	float: left;
         	line-height: 30px ;
         	color: %1 ;
         	text-indent: 15px ; /* Shift the text by 15px from the left border */
         	text-decoration: none ;
         	}
         	
         	ul#TagLinkStyle li a:hover
         	{
         	color: %2 ;
         	text-decoration:underline;
         	}
         	</style>
         ]
         
         // Colors
         gsCssHeader = StringBuild(gsCssHeader,RGBToHTML(ColorPalette(colorTextLink1,1)),RGBToHTML(ColorPalette(colorTextLink1,2)))
         
         // Header
         MyPage..HTMLHeader += gsCssHeader
